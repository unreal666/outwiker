msgstr "Заголовки HTML"

#: htmlheads.py:42
msgid ""
"Plugin adds wiki-commands (:title:), (:description:), (:keywords:) and (:"
"htmlhead:)"
msgstr ""
"Плагин добавляет команды (:title:), (:description:), (:keywords:) и (:"
"htmlhead:) для викистраниц."

#: htmlheads.py:44
msgid ""
"<b>Usage:</b>\n"
"(:title Page title:)\n"
"\n"
"(:description Page description:)\n"
"\n"
"(:keywords keyword_1, keyword_2, other keyword:)\n"
"\n"
"(:htmlhead:)\n"
"&lt;meta http-equiv='Content-Type' content='text/html; charset=utf-8' /"
"&gt;\n"
"\n"
"&lt;meta name='robots' content='index,follow' /&gt;\n"
"(:htmlheadend:)\n"
"\n"
"(:htmlattrs attr1=\"value\" attr2=\"value\":)\n"
msgstr ""
"<b>Использование:</b>\n"
"(:title Заголовок страницы:)\n"
"\n"
"(:description Описание страницы:)\n"
"\n"
"(:keywords ключевое слово 1, ключевое слово 2, еще одно ключевое слово:)\n"
"\n"
"(:htmlhead:)\n"
"&lt;meta http-equiv='Content-Type' content='text/html; charset=utf-8' /"
"&gt;\n"
"\n"
"&lt;meta name='robots' content='index,follow' /&gt;\n"
"(:htmlheadend:)\n"
"\n"
"(:htmlattrs атрибут1=\"значение\" атрибут2=\"значение\":)\n"

