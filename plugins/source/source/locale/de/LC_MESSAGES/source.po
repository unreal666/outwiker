#: plugin.py:47
msgid ""
"Add command (:source:) in wiki parser. This command highlight your source "
"code."
msgstr ""
"Den Befehl (:source:) in Wiki-Parser einfügen. Mit diesem Befehl wird der "
"Sourcecode hervorgehoben."

#: plugin.py:49
msgid ""
"<B>Usage:</B>:\n"
"(:source params... :)\n"
"source code\n"
"(:sourceend:)"
msgstr ""
"<B>Nutzung:</B>:\n"
"(:source params... :)\n"
"source code\n"
"(:sourceend:)"

#: plugin.py:54
msgid ""
"<B>Params:</B>\n"
"<U>lang</U> - programming language\n"
"\n"
"<U>tabwidth</U> - tab size\n"
"\n"
"<U>file</U> - attached source file\n"
"\n"
"<U>encoding</U> - encoding of the attached source file (default encoding - "
"utf8)\n"
"\n"
"<U>style</U> - style of hightlighting\n"
"\n"
"<U>parentbg</U> - use the page background for the code block\n"
"\n"
"<U>linenum</U> - enable line numbers"
msgstr ""
"<B>Params:</B>\n"
"<U>lang</U> - Programmiersprache\n"
"\n"
"<U>tabwidth</U> - Tab grösse\n"
"\n"
"<U>file</U> - Angehängte Sourcecodedatei\n"
"\n"
"<U>encoding</U> - Kodierung der angehängten Sourcecodedatei(Standard "
"Kodierung - utf8)\n"
"\n"
"<U>style</U> - Stil der Hervorhebung\n"
"\n"
"<U>parentbg</U> - Benutzt die Seitenhintergrundfarbe für den Code Block\n"
"\n"
"<U>linenum</U> - Zeilennummern einblenden"

#: plugin.py:69
msgid ""
"<B>Example 1:</B>\n"
"<PRE>(:source lang=\"python\" tabwidth=4:)\n"
"import os\n"
"\n"
"if __name__ == \"__main__\":\n"
"    print \"Hello World!\"\n"
"(:sourceend:)\n"
"</PRE>"
msgstr ""
"<B>Example 1:</B>\n"
"<PRE>(:source lang=\"python\" tabwidth=4:)\n"
"import os\n"
"\n"
"if __name__ == \"__main__\":\n"
"    print \"Hello World!\"\n"
"(:sourceend:)\n"
"</PRE>"

#: plugin.py:78
msgid ""
"<B>Example 2:</B>\n"
"<PRE>(:source lang=\"python\" style=\"autumn\":)\n"
"import os\n"
"\n"
"if __name__ == \"__main__\":\n"
"    print \"Hello World!\"\n"
"(:sourceend:)\n"
"</PRE>"
msgstr ""
"<B>Example 2:</B>\n"
"<PRE>(:source lang=\"python\" style=\"autumn\":)\n"
"import os\n"
"\n"
"if __name__ == \"__main__\":\n"
"    print \"Hello World!\"\n"
"(:sourceend:)\n"
"</PRE>"

#: plugin.py:87
msgid ""
"<B>Example 3:</B>\n"
"<PRE>(:source lang=\"python\" tabwidth=4 parentbg linenum:)\n"
"import os\n"
"\n"
"if __name__ == \"__main__\":\n"
"    print \"Hello World!\"\n"
"(:sourceend:)\n"
"</PRE>"
msgstr ""
"<B>Example 3:</B>\n"
"<PRE>(:source lang=\"python\" tabwidth=4 parentbg linenum:)\n"
"import os\n"
"\n"
"if __name__ == \"__main__\":\n"
"    print \"Hello World!\"\n"
"(:sourceend:)\n"
"</PRE>"

#: plugin.py:96
msgid ""
"<B>Example 4:</B>\n"
"<PRE>(:source file=\"Attach:example.cs\" encoding=\"cp1251\":)(:sourceend:)</"
"PRE>"
msgstr ""
"<B>Example 4:</B>\n"
"<PRE>(:source file=\"Attach:example.cs\" encoding=\"cp1251\":)(:sourceend:)</"
"PRE>"

#: plugin.py:99
msgid ""
"<B>Example 5:</B>\n"
"<PRE>(:source file=\"Attach:example.txt\" lang=\"python\":)(:sourceend:)</"
"PRE>"
msgstr ""
"<B>Example 5:</B>\n"
"<PRE>(:source file=\"Attach:example.txt\" lang=\"python\":)(:sourceend:)</"
"PRE>"

