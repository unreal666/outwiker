#: source.py:67
msgid ""
"Add command (:source:) in wiki parser. This command highlight your source "
"code."
msgstr ""
"Add command (:source:) in wiki parser. This command highlight your source "
"code."

#: source.py:69
msgid ""
"<B>Usage:</B>:\n"
"(:source params... :)\n"
"source code\n"
"(:sourceend:)"
msgstr ""
"<B>Usage:</B>:\n"
"(:source params... :)\n"
"source code\n"
"(:sourceend:)"

#: source.py:74
msgid ""
"<B>Params:</B>\n"
"<I>lang</I> - programming language\n"
"<I>tabwidth</I> - tab size\n"
"<I>file</I> - attached source file\n"
"<I>encoding</I> - encoding of the attached source file (default encoding - "
"utf8)\n"
"<I>style</I> - style of hightlighting\n"
"<I>parentbg</I> - use the page background for the code block"
msgstr ""
"<B>Params:</B>\n"
"<I>lang</I> - programming language\n"
"<I>tabwidth</I> - tab size\n"
"<I>file</I> - attached source file\n"
"<I>encoding</I> - encoding of the attached source file (default encoding - "
"utf8)\n"
"<I>style</I> - style of hightlighting\n"
"<I>parentbg</I> - use the page background for the code block"

#: source.py:82
msgid ""
"<B>Example 1:</B>\n"
"<PRE>(:source lang=\"python\" tabwidth=4:)\n"
"import os\n"
"\n"
"if __name__ == \"__main__\":\n"
"    print \"Hello World!\"\n"
"(:sourceend:)\n"
"</PRE>"
msgstr ""
"<B>Example 1:</B>\n"
"<PRE>(:source lang=\"python\" tabwidth=4:)\n"
"import os\n"
"\n"
"if __name__ == \"__main__\":\n"
"    print \"Hello World!\"\n"
"(:sourceend:)\n"
"</PRE>"

#: source.py:91
msgid ""
"<B>Example 2:</B>\n"
"<PRE>(:source lang=\"python\" style=\"autumn\":)\n"
"import os\n"
"\n"
"if __name__ == \"__main__\":\n"
"    print \"Hello World!\"\n"
"(:sourceend:)\n"
"</PRE>"
msgstr ""
"<B>Example 2:</B>\n"
"<PRE>(:source lang=\"python\" style=\"autumn\":)\n"
"import os\n"
"\n"
"if __name__ == \"__main__\":\n"
"    print \"Hello World!\"\n"
