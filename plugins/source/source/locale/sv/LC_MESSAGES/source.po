#: plugin.py:87
msgid ""
"<B>Example 3:</B>\n"
"<PRE>(:source lang=\"python\" tabwidth=4 parentbg linenum:)\n"
"import os\n"
"\n"
"if __name__ == \"__main__\":\n"
"    print \"Hello World!\"\n"
"(:sourceend:)\n"
